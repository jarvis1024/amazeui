{// menu/create
    "button":{
        "TEMPLATE": "root",
        "ITEM": {
            "TEMPLATE": "table",
            "name":{
                "TEMPLATE": "ctrl",
                "INPUT":{
                    "CLASS": "vj-input-single",
                    "INPUT": "input",
                    "COUNTER": "0/5",
                    "PLACEHOLDER": "按钮显示文字",
                    "TITLE": "名称",
                    "TAGNAME": true,
                    "DESC":""
                }
            },

            "type":{
                "TEMPLATE": "ctrl",
                "SELECT":{
                    "CLASS": "vj-select-row",
                    "TITLE": "类型",
                    "TEXT": "选择类型",
                    "ITEM": [
                        {"click":  "自定义事件"},
                        {"view":  "打开网页"},
                        {"scancode_push":  "扫码推事件(打开网页)"},
                        {"scancode_waitmsg":  "扫码推事件(等待消息)"},
                        {"pic_sysphoto":  "弹出拍照发图"},
                        {"pic_photo_or_album":  "弹出拍照或相册发图"},
                        {"pic_weixin":  "弹出相册发图"},
                        {"location_select":  "弹出地理位置"},
                        {"media_id":  "下发素材"},
                        {"view_limited":  "打开图文页面"}
                    ],
                    "DESC": ""
                }
            },

            "url": {
                "TEMPLATE": "ctrl",
                "INPUT":{
                    "CLASS": "vj-input-single",
                    "INPUT": "input",
                    "PLACEHOLDER": "http://example.com/url/",
                    "TITLE": "链接",
                    "TAGNAME": true,
                    "DESC":""
                }
            },

            "key": {
                "TEMPLATE": "ctrl",
                "SELECT":{
                    "CLASS": "vj-select-row",
                    "TITLE": "事件",
                    "TEXT": "选择事件组",
                    "ITEM": {"AJAX":  "http://api.mycuo.com/custom_key"},
                    "DESC": ""
                }
            },

            "media_id": {
                "TEMPLATE": "ctrl",
                "SELECT":{
                    "CLASS": "vj-file-media",
                    "TITLE": "下发素材",
                    "ITEM": {"AJAX":  "http://api.mycuo.com/media_id?t=news"},
                    "DESC": ""
                }
            },


            "sub_button":{
                "TEMPLATE": "table",
                "ITEM": {
                "TEMPLATE": "table",

                    "name":{
                        "TEMPLATE": "ctrl",
                        "INPUT":{
                            "CLASS": "vj-input-single",
                            "INPUT": "input",
                            "COUNTER": "0/15",
                            "PLACEHOLDER": "按钮显示文字",
                            "TITLE": "名称",
                            "TAGNAME": true,
                            "DESC":""
                        }
                    },

                    "type":{
                        "TEMPLATE": "ctrl",
                        "SELECT":{
                            "CLASS": "vj-select-row",
                            "TITLE": "类型",
                            "TEXT": "选择类型",
                            "ITEM": {
                                "click":  "自定义事件",
                                "view":  "打开网页",
                                "scancode_push":  "扫码推事件(打开网页)",
                                "scancode_waitmsg":  "扫码推事件(等待消息)",
                                "pic_sysphoto":  "弹出拍照发图",
                                "pic_photo_or_album":  "弹出拍照或相册发图",
                                "pic_weixin":  "弹出相册发图",
                                "location_select":  "弹出地理位置",
                                "media_id":  "下发素材",
                                "view_limited":  "打开图文页面"
                            },
                            "DESC": ""
                        }
                    },

                    "url": {
                        "TEMPLATE": "ctrl",
                        "INPUT":{
                            "CLASS": "vj-input-single",
                            "INPUT": "input",
                            "PLACEHOLDER": "http://example.com/url/",
                            "TITLE": "链接",
                            "TAGNAME": true,
                            "DESC":""
                        }
                    },

                    "key": {
                        "TEMPLATE": "ctrl",
                        "SELECT":{
                            "CLASS": "vj-select-row",
                            "TITLE": "事件",
                            "TEXT": "选择事件组",
                            "ITEM": {"AJAX":  "http://api.mycuo.com/custom_key"},
                            "DESC": ""
                        }
                    },

                    "media_id": {
                        "TEMPLATE": "ctrl",
                        "SELECT":{
                            "CLASS": "vj-file-media",
                            "TITLE": "下发素材",
                            "ITEM": {"AJAX":  "http://api.mycuo.com/media_id?t=news"},
                            "DESC": ""
                        }
                    }

                }
            }
        }
    }
}